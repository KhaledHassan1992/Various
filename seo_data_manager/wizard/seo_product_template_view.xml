<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

       <record id="seo_product_title_detail_view" model="ir.ui.view">
             <field name="name">seo.product.title.detail.view</field>
             <field name="model">seo.title.detail</field>
             <field name="arch" type="xml">
                <form string="SEOProducDetails">
                	<group col='4'>
                		<field name="name"/>
						<field name="supplier"/>
						<field name="category"/>
						<field name="description"/>						
                	</group>
                    <footer>
                        <button name="get_title_detail" string="Get Detail" type="object" class="oe_highlight"/>
                        or
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
                </form>
             </field>
         </record>

